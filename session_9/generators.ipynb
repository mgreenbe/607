   "source": [
    "def fib(max_iters=100):\n",
    "    prev = 0\n",
    "    curr = 1\n",
    "    yield curr\n",
    "    for k in range(max_iters):\n",
    "        (prev, curr) = (curr, prev + curr)\n",
    "        yield curr\n",
    "        "
   ]
